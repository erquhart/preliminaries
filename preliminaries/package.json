{
  "name": "preliminaries",
  "description": "Simple front matter parser that supports YAML, JSON and TOML.",
  "version": "2.0.0-alpha.1",
  "homepage": "https://github.com/josephearl/preliminaries",
  "author": "Joseph Earl (https://github.com/josephearl)",
  "contributors": [
    "Andrew Meyer (https://github.com/Ajedi32)",
    "Brian Woodward <brian.woodward@gmail.com> (https://github.com/doowb)",
    "Ian Storm Taylor (http://ianstormtaylor.com)",
    "Jon Schlinkert <jon.schlinkert@sellside.com> (http://twitter.com/jonschlinkert)",
    "Pawel Kadluczka (http://blog.3d-logic.com)",
    "Rob Loach <robloach@gmail.com> (http://robloach.net)"
  ],
  "repository": "josephearl/preliminaries",
  "bugs": {
    "url": "https://github.com/josephearl/preliminaries/issues"
  },
  "license": "MIT",
  "files": [
    "dist/preliminaries.js"
  ],
  "main": "dist/preliminaries.js",
  "module": "src/Preliminaries.mjs",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "flow": "flow",
    "prettier:src": "prettier --write 'src/**/*.js'",
    "prettier:test": "prettier --write 'test/**/*.js'",
    "prettier:scripts": "prettier --write '*.js'",
    "prettier": "npm run prettier:src && npm run prettier:test && npm run prettier:scripts",
    "jest": "NODE_ENV=development jest",
    "rollup:umd": "NODE_ENV=production rollup -c rollup.umd.js",
    "rollup:es": "NODE_ENV=production rollup -c rollup.es.js",
    "rollup": "npm run rollup:umd && npm run rollup:es",
    "lint": "npm run flow && npm run prettier",
    "test": "npm run jest",
    "build": "npm run rollup"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "flow-bin": "^0.44.0",
    "jest": "^19.0.2",
    "js-yaml": "^3.8.1",
    "prettier": "^1.1.0",
    "regenerator-runtime": "^0.10.3",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1"
  },
  "keywords": [
    "front",
    "matter",
    "front-matter",
    "yaml",
    "markdown",
    "toml",
    "json",
    "documentation",
    "docs",
    "parser",
    "preliminaries",
    "jekyll",
    "hugo"
  ],
  "readme": "README.md"
}
